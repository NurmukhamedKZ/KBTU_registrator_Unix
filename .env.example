KBTU_USERNAME=your_username
KBTU_PASSWORD=your_password

# UniX Platform
UNIX_EMAIL=your_unix_email
UNIX_PASSWORD=your_unix_password

# AI Configuration (Google Gemini)
GEMINI_API_KEY=your_gemini_api_key

# Database Configuration (PostgreSQL)
DATABASE_URL=postgresql://username:password@host:port/database_name

# Web App - max concurrent agents (default: 5)
# MAX_CONCURRENT_AGENTS=5

# Backend CORS (Railway backend -> allow Vercel frontend)
# Comma-separated list, example:
# FRONTEND_URL=https://your-frontend.vercel.app,http://localhost:5173
FRONTEND_URL=http://localhost:5173

# Optional redirect: if set, all non-API backend routes redirect to frontend URL.
# Example:
# FRONTEND_PUBLIC_URL=https://uni-bot-frontend-sigma.vercel.app
FRONTEND_PUBLIC_URL=
